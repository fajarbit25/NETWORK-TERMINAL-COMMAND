#Config ONU
***********
conf t
interface gpon-olt_1/1/2
onu 7 type ALL sn RTEGCA01B85D
exit

#Config Profile ONU
*******************
interface gpon-onu_1/1/2:7
tcont 5 profile 20M
gemport 1 tcont 5
gemport 1 traffic-limit upstream 20M downstream 20M
service-port 1 vport 1 user-vlan 143 vlan 143
exit

#Create WAN IP  
**************
pon-onu-mng gpon-onu_1/1/2:7
service 1 gemport 1 vlan 143
wan-ip mode pppoe username MuhammadRezaFadhilah@1730706224 password KhqNYMY2HcWtsbxD vlan-profile netmedia143 host 1
security-mgmt 212 state enable mode forward protocol web
exit


